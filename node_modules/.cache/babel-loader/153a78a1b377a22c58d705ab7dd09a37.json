,IAAqB,KAAK,CAAC,SAApD,EAA+D;AAC7D,QAAA,KAAK,CAAC,MAAN,CAAa,aAAb,EAA4B,WAAW,CAAC,KAAxC;AACF;AACD,KAJD,CAfM,CAoBN;;;AACA,QAAM,gBAAe,GAAI,SAAnB,gBAAmB,GAAM;AAC7B,UAAI,YAAY,CAAC,KAAb,CAAmB,GAAnB,IAA0B,YAAY,CAAC,KAAb,CAAmB,GAAjD,EAAsD;AACpD,QAAA,KAAK,CAAC,MAAN,CAAa,kBAAb,EAAiC,MAAM,CAAC,MAAP,CAAc,EAAd,EAAkB,YAAY,CAAC,KAA/B,CAAjC;AACF;AACD,KAJD,CArBM,CA0BN;;;AACA,QAAM,kBAAiB,GAAI,SAArB,kBAAqB,CAAC,KAAD,EAAW;AACpC,MAAA,KAAK,CAAC,MAAN,CAAa,oBAAb,EAAmC,KAAnC;AACD,KAFD,CA3BM,CA8BN;;;AACA,QAAM,oBAAmB,GAAI,SAAvB,oBAAuB,GAAM;AACjC,MAAA,kBAAkB,CAAC,KAAnB,GAA2B,CAAC,kBAAkB,CAAC,KAA/C;;AACA,UAAI,CAAC,kBAAkB,CAAC,KAAxB,EAA+B;AAC7B,QAAA,KAAK,CAAC,MAAN,CAAa,cAAb,EAA6B,iBAAiB,CAAC,KAA/C;AACF;AACD,KALD,CA/BM,CAqCN;;;AACA,QAAM,kBAAiB,GAAI,SAArB,kBAAqB,GAAM;AAC/B,MAAA,oBAAoB,CAAC,KAArB,GAA6B,CAAC,oBAAoB,CAAC,KAAnD;;AACA,UAAI,CAAC,oBAAoB,CAAC,KAA1B,EAAiC;AAC/B,QAAA,KAAK,CAAC,MAAN,CAAa,mBAAb,EAAkC,cAAc,CAAC,KAAjD;AACF;AACD,KALD;;AAOA,WAAO;AACL,MAAA,WAAW,EAAX,WADK;AAEL,MAAA,KAAK,EAAL,KAFK;AAGL,MAAA,kBAAkB,EAAlB,kBAHK;AAIL,MAAA,iBAAiB,EAAjB,iBAJK;AAKL,MAAA,YAAY,EAAZ,YALK;AAML,MAAA,oBAAoB,EAApB,oBANK;AAOL,MAAA,gBAAgB,EAAhB,gBAPK;AAQL,MAAA,kBAAkB,EAAlB,kBARK;AASL,MAAA,WAAW,EAAX,WATK;AAUL,MAAA,cAAc,EAAd,cAVK;AAWL,MAAA,oBAAoB,EAApB,oBAXK;AAYL,MAAA,kBAAkB,EAAlB;AAZK,KAAP;AAcD;AA5DY,CAAf","sourcesContent":["<template>\n  <div class=\"setting\">\n    <div class=\"container\">\n      <!-- 设置颜色 -->\n      <div class=\"meun-item\">\n        <span>主题颜色</span>\n        <span>\n          <i-input v-model=\"targetTheme\"></i-input>\n        </span>\n        <span :style=\"`color:${targetTheme}`\">@</span>\n        <span @click=\"changeTheme\">换肤</span>\n      </div>\n      <!-- 接口测试地址 -->\n      <div class=\"meun-item\">\n        <span>接口测试地址</span>\n        <span>\n          <i-input\n            v-model=\"currApiServerAddr\"\n            v-if=\"apiServerEditState\"\n          ></i-input>\n          <span v-else>{{state.serverApi}}</span>\n        </span>\n        <span @click=\"editAndSaveApiServer\">\n          {{apiServerEditState ? '保存' : '编辑'}}\n        </span>\n      </div>\n      <!-- 数据提供地址 -->\n      <div class=\"meun-item\">\n        <span>数据提供地址</span>\n        <span>\n          <i-input\n            v-model=\"dataSupplyAddr\"\n            v-if=\"dataSupplyInputState\"\n          ></i-input>\n          <span v-else>{{state.dataSupplyAddr}}</span>\n        </span>\n        <span @click=\"editDataSupplyAddr\">\n          {{dataSupplyInputState ? '保存' : '编辑'}}\n        </span>\n      </div>\n\n      <div class=\"menu-item default-value\">\n        <span>\n          <i-input\n            label=\"key\"\n            v-model=\"defaultValue.key\"\n          ></i-input>\n        </span>\n        <span>\n          <i-input\n            label=\"value\"\n            v-model=\"defaultValue.val\"\n          ></i-input>\n        </span>\n        <span>\n          <button @click=\"saveDefaultValue\">click!!!</button>\n        </span>\n        <span>\n          <ul>\n            <li\n              v-for=\"(item, index) in state.defaultValues\"\n              :key=\"index\"\n            >\n              <span>{{item.key}}:{{item.val}}</span>\n              <span @click=\"removeDefaultValue(index)\">删除</span>\n            </li>\n          </ul>\n        </span>\n      </div>\n\n    </div>\n  </div>\n</template>\n\n<script>\nimport { ref } from \"vue\";\nimport { useStore } from \"vuex\";\n\nexport default {\n  setup() {\n    const store = useStore(),\n      state = store.state,\n      currApiServerAddr = ref(\"\"),\n      targetTheme = ref(\"\"),\n      dataSupplyAddr = ref(\"\"),\n      apiServerEditState = ref(false),\n      dataSupplyInputState = ref(false),\n      defaultValue = ref({ key: \"\", val: \"\" });\n\n    currApiServerAddr.value = state.serverApi;\n    targetTheme.value = state.mainTheme;\n    dataSupplyAddr.value = state.dataSupplyAddr;\n\n    // 改变主题\n    const changeTheme = () => {\n      if (targetTheme.value && targetTheme.value != state.mainTheme) {\n        store.commit(\"changeTheme\", targetTheme.value);\n      }\n    };\n    // 设置默认值\n    const saveDefaultValue = () => {\n      if (defaultValue.value.key && defaultValue.value.val) {\n        store.commit(\"addDefaultValues\", Object.assign({}, defaultValue.value));\n      }\n    };\n    // 移除默认值\n    const removeDefaultValue = (index) => {\n      store.commit(\"removeDefaultValue\", index);\n    };\n    // 编辑api测试地址\n    const editAndSaveApiServer = () => {\n      apiServerEditState.value = !apiServerEditState.value;\n      if (!apiServerEditState.value) {\n        store.commit(\"setServerApi\", currApiServerAddr.value);\n      }\n    };\n    // 数据提供地址\n    const editDataSupplyAddr = () => {\n      dataSupplyInputState.value = !dataSupplyInputState.value;\n      if (!dataSupplyInputState.value) {\n        store.commit(\"setDataSupplyAddr\", dataSupplyAddr.value);\n      }\n    };\n\n    return {\n      changeTheme,\n      state,\n      apiServerEditState,\n      currApiServerAddr,\n      defaultValue,\n      editAndSaveApiServer,\n      saveDefaultValue,\n      removeDefaultValue,\n      targetTheme,\n      dataSupplyAddr,\n      dataSupplyInputState,\n      editDataSupplyAddr,\n    };\n  },\n};\n</script>\n\n<style scoped>\n.container {\n  position: relative;\n  width: 50%;\n  /* background-color: coral; */\n  margin: auto;\n  margin-top: 100px;\n  padding: 0 20px;\n}\n.meun-item {\n  height: 50px;\n  line-height: 50px;\n  display: flex;\n  flex-direction: row;\n  justify-content: space-between;\n  font-size: 1.2rem;\n  /* border-bottom: 2px solid slategray; */\n}\n.default-value {\n  /* background-color: violet; */\n  display: flex;\n  flex-direction: column;\n  justify-content: space-around;\n  margin-top: 20px;\n}\n</style>"],"sourceRoot":""}]}