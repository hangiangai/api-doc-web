,WAAA,EA2BM,CA1BJ,YAAA,CAKO,MALP,EAKO,IALP,EAKO,CAJL,YAAA,CAGW,kBAHX,EAGW;AAFT,IAAA,KAAK,EAAC,KAEG;gBADA,MAAA,CAAA,YAAA,CAAa,GACb;;aADA,MAAA,CAAA,YAAA,CAAa,GAAb,GAAgB,M;;AAChB,GAHX,E,IAAA,E,CAAA,E,cAAA,CAIK,CALP,CA0BI,EApBJ,YAAA,CAKO,MALP,EAKO,IALP,EAKO,CAJL,YAAA,CAGW,kBAHX,EAGW;AAFT,IAAA,KAAK,EAAC,OAEG;gBADA,MAAA,CAAA,YAAA,CAAa,GACb;;aADA,MAAA,CAAA,YAAA,CAAa,GAAb,GAAgB,M;;AAChB,GAHX,E,IAAA,E,CAAA,E,cAAA,CAIK,CALP,CAoBI,EAdJ,YAAA,CAEO,MAFP,EAEO,IAFP,EAEO,CADL,YAAA,CAAmD,QAAnD,EAAmD;AAA1C,IAAA,OAAK,EAAA,MAAA,CAAA,CAAA,CAAA,KAAA,MAAA,CAAA,CAAA,CAAA,GAAA;AAAA,aAAE,MAAA,CAAA,gBAAA,OAAA,MAAA,YAAF;AAAA,KAAA;AAAqC,GAAnD,EAAkC,UAAlC,CACK,CAFP,CAcI,EAXJ,YAAA,CAUO,MAVP,EAUO,IAVP,EAUO,CATL,YAAA,CAQK,IARL,EAQK,IARL,EAQK,E,kBAPH,YAAA,CAMK,SANL,EAMK,IANL,EAMK,WAAA,CALqB,MAAA,CAAA,KAAA,CAAM,aAK3B,EALwC,UAAnC,IAAmC,EAA7B,KAA6B,EAAxB;yBADrB,YAAA,CAMK,IANL,EAMK;AAJF,MAAA,GAAG,EAAE;AAIH,KANL,EAEa,CAEX,YAAA,CAAsC,MAAtC,EAAsC,IAAtC,EAAsC,gBAAA,CAA9B,IAAI,CAAC,GAAyB,CAAA,GAApB,GAAoB,GAAnB,gBAAA,CAAE,IAAI,CAAC,GAAP,CAAnB,EAA6B,CAA7B,CAFW,EAGX,YAAA,CAAkD,MAAlD,EAAkD;AAA3C,MAAA,OAAK,EAAA,iBAAA,MAAA;AAAA,eAAE,MAAA,CAAA,kBAAA,CAAmB,KAAnB,CAAF;AAAA;AAAsC,KAAlD,EAAyC,IAAzC,EAA2C,CAA3C,EAA2C,CAAA,SAAA,CAA3C,CAHW,CAFb,C;GAMK,CANL,E;;AAAA,GAOG,EARL,CASK,CAVP,CAWI,CA3BN,CA6BI,CApEN,CAqEI,CAtEN,C","sourcesContent":["<template>\n  <div class=\"setting\">\n    <div class=\"container\">\n      <!-- 设置颜色 -->\n      <div class=\"meun-item\">\n        <span>主题颜色</span>\n        <span>\n          <i-input v-model=\"targetTheme\"></i-input>\n        </span>\n        <span :style=\"`color:${targetTheme}`\">@</span>\n        <span @click=\"changeTheme\">换肤</span>\n      </div>\n      <!-- 接口测试地址 -->\n      <div class=\"meun-item\">\n        <span>接口测试地址</span>\n        <span>\n          <i-input\n            v-model=\"currApiServerAddr\"\n            v-if=\"apiServerEditState\"\n          ></i-input>\n          <span v-else>{{state.serverApi}}</span>\n        </span>\n        <span @click=\"editAndSaveApiServer\">\n          {{apiServerEditState ? '保存' : '编辑'}}\n        </span>\n      </div>\n      <!-- 数据提供地址 -->\n      <div class=\"meun-item\">\n        <span>数据提供地址</span>\n        <span>\n          <i-input\n            v-model=\"dataSupplyAddr\"\n            v-if=\"dataSupplyInputState\"\n          ></i-input>\n          <span v-else>{{state.dataSupplyAddr}}</span>\n        </span>\n        <span @click=\"editDataSupplyAddr\">\n          {{dataSupplyInputState ? '保存' : '编辑'}}\n        </span>\n      </div>\n\n      <div class=\"menu-item default-value\">\n        <span>\n          <i-input\n            label=\"key\"\n            v-model=\"defaultValue.key\"\n          ></i-input>\n        </span>\n        <span>\n          <i-input\n            label=\"value\"\n            v-model=\"defaultValue.val\"\n          ></i-input>\n        </span>\n        <span>\n          <button @click=\"saveDefaultValue\">click!!!</button>\n        </span>\n        <span>\n          <ul>\n            <li\n              v-for=\"(item, index) in state.defaultValues\"\n              :key=\"index\"\n            >\n              <span>{{item.key}}:{{item.val}}</span>\n              <span @click=\"removeDefaultValue(index)\">删除</span>\n            </li>\n          </ul>\n        </span>\n      </div>\n\n    </div>\n  </div>\n</template>\n\n<script>\nimport { ref } from \"vue\";\nimport { useStore } from \"vuex\";\n\nexport default {\n  setup() {\n    const store = useStore(),\n      state = store.state,\n      currApiServerAddr = ref(\"\"),\n      targetTheme = ref(\"\"),\n      dataSupplyAddr = ref(\"\"),\n      apiServerEditState = ref(false),\n      dataSupplyInputState = ref(false),\n      defaultValue = ref({ key: \"\", val: \"\" });\n\n    currApiServerAddr.value = state.serverApi;\n    targetTheme.value = state.mainTheme;\n    dataSupplyAddr.value = state.dataSupplyAddr;\n\n    // 改变主题\n    const changeTheme = () => {\n      if (targetTheme.value && targetTheme.value != state.mainTheme) {\n        store.commit(\"changeTheme\", targetTheme.value);\n      }\n    };\n    // 设置默认值\n    const saveDefaultValue = () => {\n      if (defaultValue.value.key && defaultValue.value.val) {\n        store.commit(\"addDefaultValues\", Object.assign({}, defaultValue.value));\n      }\n    };\n    // 移除默认值\n    const removeDefaultValue = (index) => {\n      store.commit(\"removeDefaultValue\", index);\n    };\n    // 编辑api测试地址\n    const editAndSaveApiServer = () => {\n      apiServerEditState.value = !apiServerEditState.value;\n      if (!apiServerEditState.value) {\n        store.commit(\"setServerApi\", currApiServerAddr.value);\n      }\n    };\n    // 数据提供地址\n    const editDataSupplyAddr = () => {\n      dataSupplyInputState.value = !dataSupplyInputState.value;\n      if (!dataSupplyInputState.value) {\n        store.commit(\"setDataSupplyAddr\", dataSupplyAddr.value);\n      }\n    };\n\n    return {\n      changeTheme,\n      state,\n      apiServerEditState,\n      currApiServerAddr,\n      defaultValue,\n      editAndSaveApiServer,\n      saveDefaultValue,\n      removeDefaultValue,\n      targetTheme,\n      dataSupplyAddr,\n      dataSupplyInputState,\n      editDataSupplyAddr,\n    };\n  },\n};\n</script>\n\n<style scoped>\n.container {\n  position: relative;\n  width: 50%;\n  /* background-color: coral; */\n  margin: auto;\n  margin-top: 100px;\n  padding: 0 20px;\n}\n.meun-item {\n  height: 50px;\n  line-height: 50px;\n  display: flex;\n  flex-direction: row;\n  justify-content: space-between;\n  font-size: 1.2rem;\n  /* border-bottom: 2px solid slategray; */\n}\n.default-value {\n  /* background-color: violet; */\n  display: flex;\n  flex-direction: column;\n  justify-content: space-around;\n  margin-top: 20px;\n}\n</style>"],"sourceRoot":""}]}