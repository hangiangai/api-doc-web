SK,CAVP,CAWI,CA3BN,CA6BI,CAnDN,CAoDI,CArDN,C","sourcesContent":["<template>\n  <div class=\"setting\">\n    <div class=\"container\">\n      <!-- 设置颜色 -->\n      <div class=\"meun-item\">\n        <span>\n          <i-input v-model=\"currApiServerAddr\"></i-input>\n        </span>\n        <span>换肤</span>\n      </div>\n      <!-- 设置apiServer -->\n      <div class=\"meun-item\">\n        <span>\n          <i-input\n            v-model=\"currApiServerAddr\"\n            v-if=\"apiServerEditState\"\n          ></i-input>\n          <span v-else>{{state.serverApi}}</span>\n        </span>\n        <span @click=\"editAndSaveApiServer\">\n          {{apiServerEditState ? '保存' : '编辑'}}\n        </span>\n      </div>\n\n      <div class=\"menu-item default-value\">\n        <span>\n          <i-input\n            label=\"key\"\n            v-model=\"defaultValue.key\"\n          ></i-input>\n        </span>\n        <span>\n          <i-input\n            label=\"value\"\n            v-model=\"defaultValue.val\"\n          ></i-input>\n        </span>\n        <span>\n          <button @click=\"saveDefaultValue\">click!!!</button>\n        </span>\n        <span>\n          <ul>\n            <li\n              v-for=\"(item, index) in state.defaultValues\"\n              :key=\"index\"\n            >\n              <span>{{item.key}}:{{item.val}}</span>\n              <span @click=\"removeDefaultValue(index)\">删除</span>\n            </li>\n          </ul>\n        </span>\n      </div>\n\n    </div>\n  </div>\n</template>\n\n<script>\nimport { ref } from \"vue\";\nimport { useStore } from \"vuex\";\n\nexport default {\n  setup() {\n    const store = useStore(),\n      state = store.state,\n      currApiServerAddr = ref(\"\"),\n      apiServerEditState = ref(false),\n      defaultValue = ref({ key: \"\", val: \"\" });\n\n    currApiServerAddr.value = state.serverApi;\n    // 改变主题\n    const changeTheme = () => {\n      document.documentElement.style.setProperty(\"--main-theme\", \"#01a299\");\n    };\n    // 设置默认值\n    const saveDefaultValue = () => {\n      if (defaultValue.value.key && defaultValue.value.val) {\n        store.commit(\"addDefaultValues\", Object.assign({}, defaultValue.value));\n      }\n    };\n    // 移除默认值\n    const removeDefaultValue = (index) => {\n      store.commit(\"removeDefaultValue\", index);\n    };\n    //\n    const editAndSaveApiServer = () => {\n      apiServerEditState.value = !apiServerEditState.value;\n      if (!apiServerEditState.value) {\n        store.commit(\"setServerApi\", currApiServerAddr.value);\n      }\n    };\n    return {\n      changeTheme,\n      state,\n      apiServerEditState,\n      currApiServerAddr,\n      defaultValue,\n      editAndSaveApiServer,\n      saveDefaultValue,\n      removeDefaultValue,\n    };\n  },\n};\n</script>\n\n<style scoped>\n.container {\n  position: relative;\n  width: 50%;\n  /* background-color: coral; */\n  margin: auto;\n  margin-top: 100px;\n  padding: 0 20px;\n}\n.meun-item {\n  height: 50px;\n  line-height: 50px;\n  display: flex;\n  flex-direction: row;\n  justify-content: space-between;\n  font-size: 1.2rem;\n  border-bottom: 2px solid slategray;\n}\n.default-value {\n  /* background-color: violet; */\n  display: flex;\n  flex-direction: column;\n  justify-content: space-around;\n  margin-top: 20px;\n}\n</style>"],"sourceRoot":""}]}