ence\"\n      @click=\"targerOpen\"\n    >\n      <slot name=\"reference\"></slot>\n    </div>\n    <div\n      v-outside=\"close\"\n      class=\"content\"\n      v-show=\"open\"\n    >\n      <slot></slot>\n    </div>\n  </div>\n</template>\n\n<script lang=\"ts\">\nimport { ref, getCurrentInstance } from \"vue\";\n\nexport default {\n  name: \"IPopover\",\n  directives: {\n    outside: {\n      mounted(el, binding) {\n        const documentHandler = function (e) {\n          if (el.contains(e.target)) return;\n          binding.value(e);\n        };\n        setTimeout(() => {\n          document.addEventListener(\"click\", documentHandler);\n        }, 300);\n      },\n    },\n  },\n  setup() {\n    const instance = getCurrentInstance(),\n      open = ref(false);\n    const targerOpen = () => {\n      open.value = !open.value;\n    };\n\n    const close = (e) => {\n      if (instance.vnode.el.contains(e.target)) return;\n      open.value = false;\n    };\n    return {\n      targerOpen,\n      open,\n      close,\n    };\n  },\n};\n</script>\n\n<style scopd>\n.popover {\n  position: relative;\n}\n.content {\n  width: 250px;\n  height: 70px;\n  background-color: royalblue;\n}\n</style>"],"sourceRoot":""}]}