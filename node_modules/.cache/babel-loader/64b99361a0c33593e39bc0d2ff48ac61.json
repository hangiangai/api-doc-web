 :key=\"index\"\n        >\n          <i-popover>\n            <template v-slot:reference>\n              <i-input\n                v-model=\"item.value\"\n                :label=\"item.name\"\n                width=\"100%\"\n                style=\"padding: 6px 0\"\n              ></i-input>\n            </template>\n            <ul>\n              <li\n                v-for=\"(value, idx) in state.defaultValues\"\n                :key=\"idx\"\n                class=\"menu-item\"\n              >\n                <span>{{value.key}}</span>\n                <span>:</span>\n                <span>{{value.val}}</span>\n                <span\n                  class=\"fill-btn\"\n                  @click=\"item.value = value.val\"\n                >填充</span>\n              </li>\n            </ul>\n          </i-popover>\n        </li>\n\n        <div\n          class=\"test-button\"\n          @click=\"startApiTest\"\n        >测试</div>\n        <div class=\"result-area\">\n          <pre>{{state.currentTestApiResult}}</pre>\n        </div>\n      </ul>\n    </div>\n  </div>\n</template>\n\n<script>\nimport { onMounted } from \"vue\";\nimport { useStore } from \"vuex\";\nimport { fetchTestResult } from \"./../services/request\";\n\nexport default {\n  setup() {\n    const store = useStore(),\n      state = store.state;\n    const startApiTest = function () {\n      let finalRequestUrl = `http://${state.serverApi}/${state.currentTestApiUrl}`;\n      fetchTestResult({\n        url: finalRequestUrl,\n        method: state.currentTestApiMethod,\n        params: state.currentTestApiParams,\n      })\n        .then((data) => {\n          store.commit(\"addTestHistory\", data);\n          store.commit(\"changeCurrentTestApiResult\", data);\n        })\n        .catch((err) => {\n          console.log(err);\n        });\n    };\n    onMounted(() => {});\n    return {\n      startApiTest,\n      state,\n    };\n  },\n};\n</script>\n\n<style scoped>\n.test-box {\n  width: 340px;\n  height: 97%;\n  position: fixed;\n  z-index: 5;\n  border-radius: 6px;\n  background-color: #f1f3f6;\n  left: 10px;\n  top: 10px;\n  overflow: hidden;\n  box-shadow: inset 0 0 15px rgba(55, 84, 170, 0),\n    inset 0 0 20px rgba(255, 255, 255, 0),\n    10px 10px 18px rgba(55, 84, 170, 0.15), -10px -10px 22px white,\n    inset 0px 0px 4px rgba(255, 255, 255, 0.2);\n  transition: box-shadow 0.7s ease-in-out;\n  transition: 0.3s;\n}\n.container {\n  width: 100%;\n  height: 100%;\n}\n.container p:first-child {\n  width: 100%;\n  height: 60px;\n  background: var(--main-theme);\n  text-align: center;\n  line-height: 60px;\n  font-weight: 600px;\n}\n.container p:first-child a {\n  color: var(--g-colorWhite);\n  font-size: 1.2rem;\n}\n.container ul {\n  padding: 10px;\n  height: calc(100% - 80px);\n  overflow-y: scroll;\n}\n.result-area {\n  height: 300px;\n  background-color: rgba(0, 0, 0, 0.1);\n  border-radius: 4px;\n  text-align: left;\n  overflow: scroll;\n  width: 100%;\n}\n.test-button {\n  height: 36px;\n  width: 100%;\n  background-color: var(--main-theme);\n  margin: 10px 0;\n  border-radius: 18px;\n  text-align: center;\n  line-height: 36px;\n  color: var(--g-colorWhite);\n  box-shadow: inset 0 0 15px rgba(55, 84, 170, 0),\n    inset 0 0 20px rgba(255, 255, 255, 0),\n    10px 10px 18px rgba(55, 84, 170, 0.15), -10px -10px 22px white,\n    inset 0px 0px 4px rgba(255, 255, 255, 0.2);\n}\n.menu-item {\n  display: flex;\n  flex-direction: row;\n  flex-wrap: wrap;\n  /* border: 1px solid black; */\n  padding: 4px 0;\n}\n.fill-btn {\n  cursor: pointer;\n  background-color: blueviolet;\n}\n</style>"],"sourceRoot":""}]}