       <span>\n          <i-input\n            v-model=\"currApiServerAddr\"\n            v-if=\"apiServerEditState\"\n          ></i-input>\n          <span v-else>{{state.serverApi}}</span>\n        </span>\n        <span @click=\"editAndSaveApiServer\">\n          {{apiServerEditState ? '保存' : '编辑'}}\n        </span>\n      </div>\n      <div class=\"menu-item default-value\">\n        <span>\n          <i-input label=\"key\"></i-input>\n        </span>\n        <span>\n          <i-input label=\"value\"></i-input>\n        </span>\n      </div>\n\n    </div>\n  </div>\n</template>\n\n<script lang=\"ts\">\nimport { ref } from \"vue\";\nimport { useStore } from \"vuex\";\n\nexport default {\n  setup() {\n    const store = useStore(),\n      state = store.state,\n      currApiServerAddr = ref(\"\"),\n      apiServerEditState = ref(false),\n      setDefaultValue = ref({\n        key: \"\",\n        val: \"\",\n      });\n\n    const changeTheme = () => {\n      document.documentElement.style.setProperty(\"--main-theme\", \"#01a299\");\n    };\n\n    const editAndSaveApiServer = () => {\n      apiServerEditState.value = !apiServerEditState.value;\n      if (!apiServerEditState.value) {\n        store.commit(\"setServerApi\", currApiServerAddr.value);\n      }\n    };\n\n    return {\n      changeTheme,\n      state,\n      apiServerEditState,\n      currApiServerAddr,\n      editAndSaveApiServer,\n      setDefaultValue,\n    };\n  },\n};\n</script>\n\n<style scoped>\n.container {\n  position: relative;\n  width: 50%;\n  /* background-color: coral; */\n  margin: auto;\n  margin-top: 100px;\n  padding: 0 20px;\n}\n.meun-item {\n  height: 50px;\n  line-height: 50px;\n  display: flex;\n  flex-direction: row;\n  justify-content: space-between;\n  font-size: 1.2rem;\n  border-bottom: 2px solid slategray;\n}\n.default-value {\n  height: 130px;\n  /* background-color: violet; */\n  display: flex;\n  flex-direction: column;\n  justify-content: space-around;\n}\n</style>"]}]}