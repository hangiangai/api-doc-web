),\n      store = useStore(),\n      state = store.state;\n\n    const test = () => {\n      requestResult = \"1233\";\n    };\n\n    const startApiTest = () => {\n      let finalRequestUrl = `http://${state.serverApi}/${state.currentTestApiUrl}`;\n      fetchTestResult({\n        url: finalRequestUrl,\n        method: state.currentTestApiMethod,\n        params: state.currentTestApiParams,\n      })\n        .then((data) => {\n          requestResult = data;\n        })\n        .catch((err) => {\n          console.log(err);\n        });\n    };\n    onMounted(() => {});\n    return {\n      state,\n      inputData,\n      startApiTest,\n      requestResult,\n      test,\n    };\n  },\n};\n</script>\n\n<style scoped>\n.test-box {\n  width: 340px;\n  height: 90%;\n  position: fixed;\n  z-index: 5;\n  border-radius: 6px;\n  background-color: #f1f3f6;\n  left: 10px;\n  top: 10px;\n  overflow: hidden;\n  box-shadow: inset 0 0 15px rgba(55, 84, 170, 0),\n    inset 0 0 20px rgba(255, 255, 255, 0),\n    10px 10px 18px rgba(55, 84, 170, 0.15), -10px -10px 22px white,\n    inset 0px 0px 4px rgba(255, 255, 255, 0.2);\n  transition: box-shadow 0.7s ease-in-out;\n}\n.container {\n  width: 100%;\n  height: 100%;\n}\n.container p:first-child {\n  width: 100%;\n  height: 60px;\n  background: goldenrod;\n  text-align: center;\n  line-height: 60px;\n  font-size: 1.2rem;\n  font-weight: 600px;\n}\n.container ul {\n  padding: 10px;\n  height: calc(100% - 80px);\n  overflow-y: scroll;\n}\n.result-area {\n  height: 300px;\n  background-color: rgba(0, 0, 0, 0.1);\n  border-radius: 4px;\n}\n.test-button {\n  height: 40px;\n  width: 100%;\n  background-color: royalblue;\n  margin: 10px 0;\n  border-radius: 3px;\n  text-align: center;\n  line-height: 40px;\n  color: #f1f3f6;\n}\n</style>"]}]}