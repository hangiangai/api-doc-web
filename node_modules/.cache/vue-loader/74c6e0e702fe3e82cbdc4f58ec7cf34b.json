ltValues\"\n              :key=\"index\"\n            >\n              <span>{{item.key}}:{{item.val}}</span>\n              <span @click=\"removeDefaultValue()\">删除</span>\n            </li>\n          </ul>\n        </span>\n      </div>\n\n    </div>\n  </div>\n</template>\n\n<script lang=\"ts\">\nimport { ref } from \"vue\";\nimport { useStore } from \"vuex\";\n\nexport default {\n  setup() {\n    const store = useStore(),\n      state = store.state,\n      currApiServerAddr = ref(\"\"),\n      apiServerEditState = ref(false),\n      defaultValue = ref({ key: \"\", val: \"\" });\n\n    // 改变主题\n    const changeTheme = () => {\n      document.documentElement.style.setProperty(\"--main-theme\", \"#01a299\");\n    };\n    // 设置默认值\n    const saveDefaultValue = () => {\n      if (defaultValue.value.key && defaultValue.value.val) {\n        store.commit(\"addDefaultValues\", Object.assign({}, defaultValue.value));\n      }\n    };\n    // 删除\n    const editAndSaveApiServer = () => {\n      apiServerEditState.value = !apiServerEditState.value;\n      if (!apiServerEditState.value) {\n        store.commit(\"setServerApi\", currApiServerAddr.value);\n      }\n    };\n    //\n    const removeDefaultValue = (key) => {\n      // TODO\n    };\n    return {\n      changeTheme,\n      state,\n      apiServerEditState,\n      currApiServerAddr,\n      defaultValue,\n      editAndSaveApiServer,\n      saveDefaultValue,\n      removeDefaultValue,\n    };\n  },\n};\n</script>\n\n<style scoped>\n.container {\n  position: relative;\n  width: 50%;\n  /* background-color: coral; */\n  margin: auto;\n  margin-top: 100px;\n  padding: 0 20px;\n}\n.meun-item {\n  height: 50px;\n  line-height: 50px;\n  display: flex;\n  flex-direction: row;\n  justify-content: space-between;\n  font-size: 1.2rem;\n  border-bottom: 2px solid slategray;\n}\n.default-value {\n  /* background-color: violet; */\n  display: flex;\n  flex-direction: column;\n  justify-content: space-around;\n  margin-top: 20px;\n}\n</style>"]}]}